# Configuration for privateinternetaccess.com WireGuard Tunnel
# Generated by pia_setup_tunnel on {{ now | date "2006-01-02 15:04:05 MST" }}
[NetDev]
Name={{ .Interface }}
Kind=wireguard

[WireGuard]
PrivateKey={{ .PrivateKey }}

# Region is {{ .Region.Id }} ({{ .Region.Name }}). The ping time at the time of
# configuration was {{ div .Region.PingTime 1000000 }}ms.
[WireGuardPeer]
Endpoint={{ .ServerIp }}:{{ .ServerPort }}
PublicKey={{ .ServerPubkey }}
AllowedIPs=0.0.0.0/0
PersistentKeepalive=25
